FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "$FOAM_CASE/system/initialConditions"

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    AMI1
    {
        type            cyclicAMI;
        value           $internalField;
    }

    AMI2
    {
        type            cyclicAMI;
        value           $internalField;
    }
    
    inlet
    {
        type            fixedValue;
        value           uniform $flowVelocity;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    "turbine_rotor.*"
    {
        type            fixedValue;
        value           $internalField;
    }

    "turbine_comp.*"
    {
        type            noSlip;
    }

    walls
    {
        type            slip;
    }
}


// ************************************************************************* //
