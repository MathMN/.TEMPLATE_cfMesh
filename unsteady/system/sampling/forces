forces
{
    type          forces;
    libs          (forces);

    writeControl  writeTime;
    timeInterval  1;

    log           yes;

    patches       ("turbine_rotor.*");
    rho           rhoInf;     // Indicates incompressible
    log           true;
    rhoInf        1.225;        // Redundant for incompressible

    CofR          ( 0 0 0);    // Rotation around centre line of propeller
    pitchAxis     (-1 0 0);
}


